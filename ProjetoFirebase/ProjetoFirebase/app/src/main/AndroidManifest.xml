<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.projetofirebase"
    xmlns:dist="http://schemas.android.com/apk/distribution">

    <dist:module dist:instant="true" />
    <application
        android:debuggable="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_padlock"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_padlock_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ProjetoFirebase"
        tools:ignore="HardcodedDebugMode">
        <activity android:name=".FormCadastro"></activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".TelaPrincipal"/>
        <activity android:name=".FormLogin">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>